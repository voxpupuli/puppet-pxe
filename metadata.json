{
  "name": "puppet-pxe",
  "version": "1.0.1",
  "author": "Vox Pupuli",
  "summary": "A Puppet module to deploy and manage a Pre eXecution Environment",
  "license": "Apache-2.0",
  "source": "git://github.com/voxpupuli/puppet-pxe.git",
  "project_page": "https://github.com/voxpupuli/puppet-pxe",
  "issues_url": "https://github.com/voxpupuli/puppet-pxe/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "FreeBSD",
      "operatingsystemrelease": [
        "10",
        "9.3",
        "8.4"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 5.5.8 < 7.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 4.1.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.25.0 < 7.0.0"
    },
    {
      "name": "puppetlabs/tftp",
      "version_requirement": ">= 0.2.0 < 2.0.0"
    }
  ]
}
